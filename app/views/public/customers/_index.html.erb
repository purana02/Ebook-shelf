<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>登録日</th>
      <th>タイトル</th>
      <th>購入したサイト名</th>
    </tr>
  </thead>
  <tbody>
    <% havings.each do |having| %>
    <tr>
      <td>
        <%= link_to comic_path(having.comic_id) do %>
        <%= having.created_at.strftime('%Y/%m/%d') %>
        <% end %>
      </td>
      <td><%= having.comic.title %></td>
      <td><%= having.site.name %></td>
    </tr>
    <% end %>
  </tbody>
</table>