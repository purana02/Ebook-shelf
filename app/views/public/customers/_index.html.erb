<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>投稿日</th>
      <th>タイトル</th>
      <th>購入したサイト名</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% comics.each do |comic| %>
    <tr>
      <td><%= comic.created_at.strftime('%Y/%m/%d') %></td>
      <td><%= comic.name %></td>
      <td><%= comic.site.name %></td>
    </tr>
  </tbody>
</table>