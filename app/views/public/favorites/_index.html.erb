<div class="col-md-10">
<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>タイトル</th>
      <th>平均</th>
      <th>レビュー数</th>
    </tr>
  </thead>
  <tbody>
    <% favorites.each do |favorite| %>
    <tr>
      <td>
        <%= link_to comic_path(favorite.comic_id) do %>
        <%= favorite.comic.title %>
        <% end %>
      </td>
      <td><!--%= render 'public/reviews/average_star', comic: comic %--></td>
      <td><%= favorite.comic.reviews.count %>件</td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>